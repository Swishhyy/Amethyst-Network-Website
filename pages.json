{
  "build": {
    "command": "npm install && npm run build",
    "publish": "_site",
    "environment": {
      "NODE_VERSION": "18"
    }
  },
  "processing": {
    "html": {
      "pretty_urls": true
    },
    "css": {
      "bundle": true,
      "minify": true
    },
    "js": {
      "bundle": true,
      "minify": true
    },    "images": {
      "compress": false,
      "optimize": false,
      "webp": false
    }
  }
}
